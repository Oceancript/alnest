      ".footer-block h5,.footer-block a,.footer-block p,.footer-block__details-content p {color: #848484 !important;}",
      ".footer-block .list-menu__item--link {padding: 0px;}",
      ".footer-block__details-content h5:first-child,.footer-block__details-content h5 {margin: 0 !important;}",
      "@media screen and (min-width: 750px) {.custom-banner {margin: 0 5rem; }}",
      "@media screen and (min-width: 1200px) {.footer-block:first-child {display: block !important; } .footer-block:last-child {margin-top: -20px !important; }}",
      "@media screen and (min-width: 1740px) {.custom-banner {margin: 0 auto; }}",
      ".footer-block:last-child {grid-column: span 2; text-align: center;}",
      ".footer__blocks-wrapper {display: grid; justify-content: space-between; grid-template-columns: repeat(2, 1fr);}",
      ".footer-block {margin: 0;}",
      ".footer-block {width: auto;}",
      "@media screen and (min-width: 600px) {.footer__blocks-wrapper {row-gap: 50px; grid-template-columns: repeat(2, 1fr); } .footer__content-top {padding: 0 5.5rem; }}",
      "@media screen and (min-width: 1200px) {.footer__blocks-wrapper {row-gap: 0px !important; } .footer__blocks-wrapper {grid-template-columns: auto auto auto auto auto; } .footer-block + .footer-block {grid-row: span 2; } .footer-block:last-child {grid-row: span 1 !important; grid-column: span 1; text-align: left; }}",
